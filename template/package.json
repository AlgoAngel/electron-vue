{
  "name": "{{ name }}",
{{#if_eq builder 'packager'}}
  "productName": "{{ name }}",
{{/if_eq}}
  "version": "0.0.0",
  "author": "{{ author }}",
  "description": "{{ description }}",
  "license": null,
  "main": "./dist/electron/main.js",
  "scripts": {
    {{#if_eq builder 'packager'}}
    "build": "node .electron-vue/build.js",
    "build:darwin": "cross-env BUILD_TARGET=darwin node .electron-vue/build.js",
    "build:linux": "cross-env BUILD_TARGET=linux node .electron-vue/build.js",
    "build:mas": "cross-env BUILD_TARGET=mas node .electron-vue/build.js",
    "build:win32": "cross-env BUILD_TARGET=win32 node .electron-vue/build.js",
    {{else}}
    "build": "yarn compile && electron-builder",
    "build:dir": "yarn build -- --dir -c.compression=store -c.mac.identity=null",
    {{/if_eq}}
    "build:clean": "del dist/electron/* !.gitkeep",
    "build:web": "webpack --env.production --config .electron-vue webpack.web.config.js",
    "compile": "electron-webpack",
    "dev": "electron-webpack dev",
    {{#if e2e}}
    "e2e": "npm run pack && mocha test/e2e",
    {{/if}}
    {{#if eslint}}
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src{{#testing unit e2e}} test{{/testing}}",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src{{#testing unit e2e}} test{{/testing}}",
    {{/if}}
    {{#testing unit e2e}}
    "test": "{{#unit}}npm run unit{{/unit}}{{#unit}}{{#e2e}} && {{/e2e}}{{/unit}}{{#e2e}}npm run e2e{{/e2e}}",
    {{/testing}}
    {{#if unit}}
    "unit": "karma start test/unit/karma.conf.js",
    {{/if}}
    "postinstall": "{{#if eslint}}npm run lint:fix{{/if}}"
  },
  {{#if_eq builder 'builder'}}
  "build": {
    "productName": "{{ name }}",
    "appId": "org.simulatedgreg.electron-vue",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
{{/if_eq}}
  "dependencies": {
    "vue": "^2.3.3",
    "source-map-support": "^0.4.17"{{deps plugins}}
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.5",
    "del-cli": "^1.1.0",
    "electron": "^1.7.5",
    "electron-debug": "^1.4.0",
    "electron-webpack": "^1.4.2",
    "electron-webpack-vue": "^1.0.7",
    {{#if_eq builder 'packager'}}
    "electron-packager": "^8.5.0",
    {{else}}
    "electron-builder": "^19.19.1",
    {{/if_eq}}
    {{#eslint}}
    "electron-webpack-eslint": "^1.0.3",
    "eslint-plugin-html": "^3.1.1",
    {{#if_eq eslintConfig 'standard'}}
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    {{/if_eq}}
    {{#if_eq eslintConfig 'airbnb'}}
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    {{/if_eq}}
    {{/eslint}}
    {{#if unit}}
    "inject-loader": "^3.0.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.1.1",
    "karma-mocha": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.1",
    "webpack-merge": "^4.1.0",
    {{/if}}
    {{#if e2e}}
    "require-dir": "^0.3.0",
    "spectron": "^3.7.1",
    {{/if}}
    {{#testing unit e2e}}
    "babel-plugin-istanbul": "^4.1.1",
    "chai": "^4.0.0",
    "mocha": "^3.0.2",
    {{/testing}}
    "vue-loader": "^12.2.2",
    "webpack": "^3.5.2"
  }
}
